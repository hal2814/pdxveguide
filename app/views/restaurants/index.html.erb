<% @restaurants.each do |restaurant| %>
<div class="row">
  <div class="col s4 listing-card-med">
    <div class="card">
      <div class="card-image">
        <%= image_tag("restaurant.jpg", class: "listing-image") %>
        <div class="card-content">
          <h4><%= link_to restaurant.title, restaurant_path(restaurant.id) %></h4>
          <p><%= restaurant.restaurant_type %></p>
        </div>
      </div>
    </div>
  </div>

  <div class="col s4">
    <div class="card ">
      <div class="card-content">
        <p><%= restaurant.website %></p>
        <p><%= restaurant.online_menu %></p>
      </div>
    </div>
  </div>
  <div class="col s4">
    <div class="card">
      <div class="card-content">
        <p><%= restaurant.location %></p>
        <p><%= restaurant.hours %></p>
      </div>
    </div>
  </div>
</div>
<% end %>

<%= will_paginate @restaurants %>
<br>
